<div class="dark-bg">
  <div class="container">
    <h2>
      {{if eq .Title "Individuals"}}
        <span class="glyphicon glyphicon-tower" aria-hidden="true"></span>
      {{else}}
        <span class="glyphicon glyphicon-music" aria-hidden="true"></span>
      {{end}}
      {{.Title}}
    </h2>

    <div class="row browse-az">
        <div><div>
    {{range .ListingSummaries}}
      {{if .Sort}}
        </div></div>
        <div class="row letter">
          <div class="col-sm-3">
            <h3>{{.Sort}}</h3>
          </div>
          <div class="col-sm-9">
            <h4 class="first"><a href="/listing/{{.Id}}">{{.Name}}</a></h4>
          </div>
          <div class="col-sm-offset-3 col-sm-9">
      {{else}}
            <h4><a href="/listing/{{.Id}}">{{.Name}}</a></h4>
      {{end}}
    {{end}}
        </div>
  </div>
</div>
